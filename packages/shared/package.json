{
  "name": "@gouv-widgets/shared",
  "version": "0.1.0",
  "description": "Utilitaires partag√©s pour les apps gouv-widgets",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    },
    "./constants/*": {
      "import": "./dist/constants/*.js",
      "types": "./dist/constants/*.d.ts"
    },
    "./api/*": {
      "import": "./dist/api/*.js",
      "types": "./dist/api/*.d.ts"
    },
    "./storage/*": {
      "import": "./dist/storage/*.js",
      "types": "./dist/storage/*.d.ts"
    },
    "./ui/*": {
      "import": "./dist/ui/*.js",
      "types": "./dist/ui/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
